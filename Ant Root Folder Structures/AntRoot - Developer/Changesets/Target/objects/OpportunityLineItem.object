        <fullName>Project_Awaiting_Onboard_to_today__c</fullName>
        <externalId>false</externalId>
        <formula>IF(DATEVALUE(Project_Start_Date__c) &lt;&gt; TODAY(),
  /* If TRUE */
     IF(AND((CASE(MOD(Today() - DATE(1985, 1, 5), 7), 0, 0, 1, 0, 2, 1, 3, 2, 4, 3, 5, 4, 6, 5, 0)+ CASE(MOD(DATEVALUE(Project_Start_Date__c) - DATE(1985, 1, 5), 7), 0, 0, 1,5, 2, 5, 3, 4, 4, 3, 5, 2, 6, 1, 0)&gt;5),Today() - DATEVALUE(Project_Start_Date__c) &lt; 6 ),CASE(MOD(Today() - DATE(1985, 1, 5), 7), 0, 0, 1, 0, 2, 1, 3, 2, 4, 3, 5, 4, 6, 5, 0)+ CASE(MOD(DATEVALUE(Project_Start_Date__c) - DATE(1985, 1, 5), 7), 0, 0, 1,5, 2, 5, 3, 4, 4, 3, 5, 2, 6, 1, 0)- 6,FLOOR((Today()-(DATEVALUE(Project_Start_Date__c)+ CASE(MOD(DATEVALUE(Project_Start_Date__c) - DATE(1985, 1, 5), 7), 0, 0, 1,6, 2, 5, 3, 4, 4, 3, 5, 2, 6, 1, 0)))/7)*5+CASE(MOD(DATEVALUE(Project_Start_Date__c) - DATE(1985, 1, 5), 7), 0, 0, 1,5, 2, 5, 3, 4, 4, 3, 5, 2, 6, 1, 0)+CASE(MOD(Today() - DATE(1985, 1, 5), 7), 0, 0, 1, 0,2, 1, 3, 2, 4, 3, 5, 4, 6, 5, 0)-1),
  /* If FALSE */
0)</formula>
        <formulaTreatBlanksAs>BlankAsZero</formulaTreatBlanksAs>
        <label>Project Awaiting Onboard to today</label>
