    <fields>
        <fullName>Project_In_Pipe_Time__c</fullName>
        <description>Calculates the number of business days between the Project Start Date and the Live Date. The Project Start Date should come after the Live Date. If the Project Start Date is before the LIve Date, the value is zero.</description>
        <externalId>false</externalId>
        <formula>IF(DATEVALUE(Project_Start_Date__c)&lt;DATEVALUE(Live_Date__c),0,

/* First-Order Calculation, subtract the dates */
DateValue(Project_Start_Date__c)- DateValue(Live_Date__c)
/* Second-Order calculation, remove the weekends */
- (FLOOR(( DateValue(Project_Start_Date__c)- DateValue(Live_Date__c))/7)*2)
/* Third-order calculation, adjust for rounding errors based on days of week */
- IF((MOD( DateValue(Live_Date__c)          - DATE(2000,1,2),7) = 6
   &amp;&amp; MOD( DateValue(Project_Start_Date__c) - DATE(2000,1,2),7) != 6)
|| (MOD( DateValue(Live_Date__c)-             DATE(2000,1,2),7) != 6 
 &amp;&amp; MOD( DateValue(Project_Start_Date__c) - DATE(2000,1,2),7) = 6), 1,
IF(MOD(DateValue(Live_Date__c)- DATE(2000,1,2),7) &gt; MOD( DateValue(Project_Start_Date__c) - DATE(2000,1,2),7), 2, 0)) 

)</formula>
        <formulaTreatBlanksAs>BlankAsZero</formulaTreatBlanksAs>
