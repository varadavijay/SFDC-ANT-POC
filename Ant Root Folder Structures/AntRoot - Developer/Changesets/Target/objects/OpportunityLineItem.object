    <fields>
        <fullName>Bus_Days_OnBoarded__c</fullName>
        <externalId>false</externalId>
        <formula>IF( 
MOD((  OnBoard_Date__c ) - DATE(2000,1,4),7)=0, 
IF(Today() - OnBoard_Date__c &lt;= 3,Today() - OnBoard_Date__c ,(Today() - OnBoard_Date__c ) - ((Today() - OnBoard_Date__c)/7) * 2), 


IF(MOD((OnBoard_Date__c ) - DATE(2000,1,5),7)=0, 
IF(Today() - OnBoard_Date__c &lt;= 2,Today() - OnBoard_Date__c ,(Today() - OnBoard_Date__c ) - (FLOOR(Today() - OnBoard_Date__c )/7) * 2), 


IF(MOD((OnBoard_Date__c ) - DATE(2000,1,6),7)=0, 
IF(Today() - OnBoard_Date__c &lt;= 1,Today() - OnBoard_Date__c ,(Today() - OnBoard_Date__c ) - (FLOOR(Today() - OnBoard_Date__c )/7) * 2), 


IF(MOD((OnBoard_Date__c ) - DATE(2000,1,7),7)=0, 
IF(Today() - OnBoard_Date__c = 0,Today() - OnBoard_Date__c ,(Today() - OnBoard_Date__c ) - (FLOOR(Today() - OnBoard_Date__c )/7) * 2), 


IF(MOD((OnBoard_Date__c ) - DATE(2000,1,8),7)=0, 
IF(Today() - OnBoard_Date__c &lt; 3,0,(Today() - OnBoard_Date__c ) - (FLOOR(Today() - OnBoard_Date__c )/7) * 2), 


IF(MOD((OnBoard_Date__c ) - DATE(2000,1,9),7)=0, 
IF(Today() - OnBoard_Date__c &lt; 2,0,(Today() - OnBoard_Date__c ) - (FLOOR(Today() - OnBoard_Date__c )/7) * 2), 


IF(Today() - OnBoard_Date__c &lt; 5,(Today() - OnBoard_Date__c ),(Today() - OnBoard_Date__c ) - (FLOOR(Today() - OnBoard_Date__c )/7) * 2) + ((FLOOR(Today() - OnBoard_Date__c )/7)-1) 
) 
) 
) 
) 
) 
)</formula>
        <formulaTreatBlanksAs>BlankAsZero</formulaTreatBlanksAs>
        <label>Bus Days (OnBoarded)</label>
        <precision>18</precision>
        <required>false</required>
        <scale>0</scale>
        <type>Number</type>
        <unique>false</unique>
    </fields>
