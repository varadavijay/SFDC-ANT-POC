    <validationRules>
        <fullName>CancelReason_Required_On_Cancel</fullName>
        <active>true</active>
        <errorConditionFormula>AND( 
Opportunity.RecordType.Name != &apos;USCP Opportunity&apos;,  
OR(
ISCHANGED(Cancellation_Category__c),
ISCHANGED(Cancellation_Reason__c)
),
!ISPICKVAL(Cancellation_Category__c,&quot;&quot;),
ISPICKVAL(Cancellation_Reason__c,&quot;&quot;)
)</errorConditionFormula>
        <errorDisplayField>Cancellation_Reason__c</errorDisplayField>
        <errorMessage>Please select a Cancellation Reason from the picklist</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Cancel_NonRenew_Status_Requirements</fullName>
        <active>true</active>
        <errorConditionFormula>AND(
Opportunity.RecordType.Name != &apos;USCP Opportunity&apos;, 
OR(
ISCHANGED(Waiting_Status__c),  
ISCHANGED(Cancellation_Category__c)
),
OR(
ISPICKVAL(Waiting_Status__c,&quot;Cancelled&quot;),
ISPICKVAL(Waiting_Status__c,&quot;Fulfilled Non-Renewal&quot;)
),
OR(
ISBLANK(Waiting_Status_Descr__c),
ISBLANK(TEXT(Cancellation_Category__c)))
)</errorConditionFormula>
        <errorDisplayField>Waiting_Status__c</errorDisplayField>
        <errorMessage>Cancelled or Non-Renewal requires a Cancellation Category selection and Status Descr/Notes</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Cancel_Removal</fullName>
        <active>true</active>
        <errorConditionFormula>AND(
Opportunity.RecordType.Name != &apos;USCP Opportunity&apos;,
NOT(
OR(
ISPICKVAL(Waiting_Status__c,&quot;Canceled&quot;),
ISPICKVAL(Waiting_Status__c,&quot;Cancelled&quot;),
ISPICKVAL(Waiting_Status__c,&quot;Fulfilled Non-Renewal&quot;)
)),
NOT(ISBLANK(TEXT(Cancellation_Category__c)))
)</errorConditionFormula>
        <errorDisplayField>Cancellation_Category__c</errorDisplayField>
        <errorMessage>Please Clear Cancellation Category if there is not A Canceled or Non-Renewal Status</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Cancel_Requires_Notes</fullName>
        <active>true</active>
        <errorConditionFormula>AND(
Opportunity.RecordType.Name != &apos;USCP Opportunity&apos;, 
OR(
ISCHANGED(Waiting_Status__c),
ISCHANGED(Waiting_Status_Descr__c )),
OR(
ISPICKVAL(Waiting_Status__c,&quot;Cancelled&quot;),
ISPICKVAL(Waiting_Status__c,&quot;Canceled&quot;)
),
ISBLANK(Waiting_Status_Descr__c ))</errorConditionFormula>
        <errorDisplayField>Waiting_Status_Descr__c</errorDisplayField>
        <errorMessage>Notes Are Required For A Cancelled Status</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Cancelled_Status_needs_Closed_Won_Stage</fullName>
        <active>true</active>
        <errorConditionFormula>IF(NOT(ISPICKVAL(Opportunity.StageName, &quot;Closed Won&quot;)) &amp;&amp;  ISPICKVAL( Waiting_Status__c , &quot;Cancelled&quot;) , true, false)</errorConditionFormula>
        <errorMessage>Opportunity Products can not be Cancelled if the Opportunity is in a stage other than Closed Won. Please mark the Opportunity as Closed Won in order to cancel the Opportunity Products or contact your manager.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Cannot_Back_Date_Live_Date</fullName>
        <active>false</active>
        <errorConditionFormula>AND(Live_Date__c &lt; (NOW() - (1/100)), NOT(ISNULL(Live_Date__c)),ISCHANGED( Live_Date__c ))</errorConditionFormula>
        <errorDisplayField>Live_Date__c</errorDisplayField>
        <errorMessage>You cannot back date a Live Date Value.  Please refer to your manager to set prior Live Dates</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Cannot_Enter_Live_Date_Unchecked</fullName>
        <active>false</active>
        <errorConditionFormula>AND(NOT(ISNULL(Live_Date__c)), Live__c = False,ISCHANGED( Live_Date__c ))</errorConditionFormula>
        <errorDisplayField>Live__c</errorDisplayField>
        <errorMessage>You must check the &apos;Live&apos; checkbox to set the Live Date.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Dig_AM_Requirement_for_Status</fullName>
        <active>false</active>
        <errorConditionFormula>AND( Opportunity.Dig_Account_Manager__c = Null, NOT(ISPICKVAL(Waiting_Status__c,&quot;&quot;)),Opportunity.Digital_Products_Notice__c = True, NOT(Opportunity.RecordTypeId=&quot;012F0000000y9rk&quot;))</errorConditionFormula>
        <errorDisplayField>Waiting_Status__c</errorDisplayField>
        <errorMessage>You Must Have A Digital Account Manager Assigned To The Opportunity Prior To Assigning A Status To Any Product.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>IssueNotes_Required_On_Cancel</fullName>
        <active>true</active>
        <errorConditionFormula>AND( 
Opportunity.RecordType.Name != &apos;USCP Opportunity&apos;, 
OR(
ISNULL(Waiting_Status_Descr__c),
Waiting_Status_Descr__c=&quot;Empty&quot;
),
ISPICKVAL(Waiting_Status__c ,&quot;Cancelled&quot;)
)</errorConditionFormula>
        <errorDisplayField>Waiting_Status_Descr__c</errorDisplayField>
        <errorMessage>Please Enter a Description of the Cancelation Reason/s</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>IssueNotes_Required_On_Waiting</fullName>
        <active>true</active>
        <errorConditionFormula>AND(
Waiting_Status_Descr__c = Null,
OR(
ISPICKVAL( Waiting_Status__c ,&quot;Waiting On Client Resource&quot;),
ISPICKVAL(Waiting_Status__c,&quot;Waiting On Client Approval&quot;)
)
)</errorConditionFormula>
        <errorDisplayField>Waiting_Status_Descr__c</errorDisplayField>
        <errorMessage>Please Enter a Description of the Waiting Reason</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>List_Change_Permissions</fullName>
        <active>false</active>
        <description>Only allow changes to prices on Setup product or Component Product Family</description>
        <errorConditionFormula>and
(ISPICKVAL(Product2.Family, &quot;&quot;),
( Product2.Name &lt;&gt; &quot;Setup Fee&quot;))</errorConditionFormula>
        <errorDisplayField>UnitPrice</errorDisplayField>
        <errorMessage>Sorry, pricing modifications are only allowed on setup or components.  Contact your manager.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Non_USCP_Cannot_Select_HardCancel</fullName>
        <active>true</active>
        <errorConditionFormula>AND(
Opportunity.RecordType.Name != &apos;USCP Opportunity&apos;,
ISCHANGED(Waiting_Status__c),
ISPICKVAL(Waiting_Status__c,&quot;Hard Cancel&quot;)
)</errorConditionFormula>
        <errorDisplayField>Waiting_Status__c</errorDisplayField>
        <errorMessage>Hard Cancel is not a valid option for this Opportunity type</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Only_Admin_change_Live_Date</fullName>
        <active>true</active>
        <description>Only Admins are allowed to change live dates</description>
        <errorConditionFormula>ISCHANGED( Live_Date__c )
&amp;&amp; !ISNULL(Live_Date__c) 
&amp;&amp; !Contains($Profile.Name,&apos;Admin&apos;)</errorConditionFormula>
        <errorDisplayField>Live_Date__c</errorDisplayField>
        <errorMessage>Only Admins have access to change the live date.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Pending_Requires_Notes</fullName>
        <active>true</active>
        <errorConditionFormula>AND(ISPICKVAL(Waiting_Status__c,&quot;Pending&quot;),ISBLANK( Waiting_Status_Descr__c ),DATEVALUE(CreatedDate)&gt;=DATE(2013,3,28))</errorConditionFormula>
        <errorDisplayField>Waiting_Status_Descr__c</errorDisplayField>
        <errorMessage>Notes Are Required For A Pending Status.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Quantity_Must_Be_An_Integer</fullName>
        <active>true</active>
        <description>The quantity field (which translates into periods) can only be an integer. Decimals are not allowed.</description>
        <errorConditionFormula>FLOOR( Quantity ) &lt;&gt;  Quantity</errorConditionFormula>
        <errorDisplayField>Quantity</errorDisplayField>
        <errorMessage>Please enter a whole number for Periods.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Setup_Fee_List_Change_OK_Nothing_Else</fullName>
        <active>false</active>
        <errorConditionFormula>AND( 
ListPrice &lt;&gt; UnitPrice ,
ISPICKVAL(Product2.Family,&quot;&quot;) , 
HasRevenueSchedule =FALSE


)</errorConditionFormula>
        <errorDisplayField>UnitPrice</errorDisplayField>
        <errorMessage>Sorry, pricing modifications are not allowed on items that are not components or setup up fees. Contact your manager.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Seven_Days_Start_Date_Force</fullName>
        <active>false</active>
        <errorConditionFormula>AND(NOT(Opportunity.IsClosed), ServiceDate&lt;(TODAY()+7))</errorConditionFormula>
        <errorDisplayField>ServiceDate</errorDisplayField>
        <errorMessage>Start Date cannot be less than 7 days from Today</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Start_Date_Force</fullName>
        <active>true</active>
        <errorConditionFormula>AND(ISNULL(ServiceDate), Product2.Digital_No_Start_Date_Req__c  = False )</errorConditionFormula>
        <errorDisplayField>ServiceDate</errorDisplayField>
        <errorMessage>You must enter a Start Date</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Start_Date_Force_Old</fullName>
        <active>false</active>
        <errorConditionFormula>ISNULL(ServiceDate)</errorConditionFormula>
        <errorDisplayField>ServiceDate</errorDisplayField>
        <errorMessage>You must enter a Start Date</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>USCP_CancelDate_Removal</fullName>
        <active>true</active>
        <description>If we remove Invoice Cancel Date for USCP Opportunity Product then remove Waiting Status Desc ,Cancellation Category and Cancellation reason</description>
        <errorConditionFormula>AND( 
Opportunity.RecordType.Name == &apos;USCP Opportunity&apos;, 
ISBLANK(Invoice_CancelDate__c),
OR(
NOT(ISBLANK(TEXT(Cancellation_Category__c))),
NOT(ISBLANK(TEXT(Cancellation_Reason__c))) 
) 
)</errorConditionFormula>
        <errorMessage>Please clear the selected Cancellation Reason and Cancellation Category.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>USCP_CancelDate_Requires_Notes_Category</fullName>
        <active>true</active>
        <description>If there is Invoice Cancel Date on USCP Opportunity Product then Notes , Cancellation Category and Cancellation Reason are required.</description>
        <errorConditionFormula>AND( 
Opportunity.RecordType.Name == &apos;USCP Opportunity&apos;, 
NOT(ISBLANK(Invoice_CancelDate__c)),  
OR( 
ISBLANK(Waiting_Status_Descr__c), 
ISBLANK(TEXT(Cancellation_Category__c)),
ISBLANK(TEXT(Cancellation_Reason__c))  
)
)</errorConditionFormula>
        <errorMessage>Please enter data in all fields.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>USCP_InvoiceCancelDate_Min_Date</fullName>
        <active>true</active>
        <errorConditionFormula>AND(
Opportunity.RecordType.Name == &apos;USCP Opportunity&apos;,
ISCHANGED(Invoice_CancelDate__c), 
Invoice_CancelDate__c &lt; Today()
)</errorConditionFormula>
        <errorMessage>Please Enter a date of today or in the future.</errorMessage>
    </validationRules>
