        <description>+ ((FLOOR((Today() -  Status_Change_Date__c)-2)/7))</description>
        <externalId>false</externalId>
        <formula>IF( 
MOD(( Status_Change_Date__c ) - DATE(2000,1,4),7)=0,
IF(Today() -  Status_Change_Date__c &lt;= 3,Today() -  Status_Change_Date__c,(Today() -  Status_Change_Date__c) - (FLOOR(Today() -  Status_Change_Date__c)/7) * 2) + 1, 


IF(MOD((Status_Change_Date__c ) - DATE(2000,1,5),7)=0,
IF(Today() -  Status_Change_Date__c &lt;= 2,Today() -  Status_Change_Date__c,(Today() -  Status_Change_Date__c) - (FLOOR(Today() -  Status_Change_Date__c)/7) * 2), 


IF(MOD((Status_Change_Date__c ) - DATE(2000,1,6),7)=0,
IF(Today() -  Status_Change_Date__c &lt;= 1,Today() -  Status_Change_Date__c,(Today() -  Status_Change_Date__c) - (FLOOR(Today() -  Status_Change_Date__c)/7) * 2), 


IF(MOD((Status_Change_Date__c ) - DATE(2000,1,7),7)=0,
IF(Today() -  Status_Change_Date__c = 0,Today() -  Status_Change_Date__c,(Today() -  Status_Change_Date__c) - (FLOOR(Today() -  Status_Change_Date__c)/7) * 2), 


IF(MOD((Status_Change_Date__c ) - DATE(2000,1,8),7)=0,
IF(Today() -  Status_Change_Date__c &lt; 3,0,(Today() -  Status_Change_Date__c) - (FLOOR(Today() -  Status_Change_Date__c)/7) * 2), 


IF(MOD((Status_Change_Date__c ) - DATE(2000,1,9),7)=0,
IF(Today() -  Status_Change_Date__c &lt; 2,0,(Today() -  Status_Change_Date__c) - (FLOOR(Today() -  Status_Change_Date__c)/7) * 2), 


IF(Today() -  Status_Change_Date__c &lt; 5,(Today() -  Status_Change_Date__c),(Today() -  Status_Change_Date__c) - (FLOOR(Today() -  Status_Change_Date__c)/7) * 2) + ((FLOOR(Today() -  Status_Change_Date__c)/7)-1)
) 
) 
) 
) 
) 
)</formula>
        <formulaTreatBlanksAs>BlankAsZero</formulaTreatBlanksAs>
