    <fields>
        <fullName>DaysInStatus_InFulfillment2__c</fullName>
        <description>Days in Status In Fulfillment for business days only (no weekends)</description>
        <externalId>false</externalId>
        <formula>IF(NOT(ISBLANK( In_Fulfillment_Out_Stamp__c )), 

/* If TRUE */ 
IF( 
/* Is same business week? */ 
AND( 
(CASE(MOD(DATEVALUE(In_Fulfillment_Out_Stamp__c ) - DATE(1985, 1, 5), 7), 0, 0, 1, 0, 2, 1, 3, 2, 4, 3, 5, 4, 6, 5, 0) 
+ CASE(MOD( Project_In_Fulfill_Date__c - DATE(1985, 1, 5), 7), 0, 0, 1, 
5, 2, 5, 3, 4, 4, 3, 5, 2, 6, 1, 0) 
&gt;5), 
DATEVALUE(In_Fulfillment_Out_Stamp__c ) - Project_In_Fulfill_Date__c &lt; 6 ), 
/* Then work out the number of business days with this formula */ CASE(MOD(DATEVALUE(In_Fulfillment_Out_Stamp__c ) - DATE(1985, 1, 5), 7), 0, 0, 1, 0, 2, 1, 3, 2, 4, 3, 5, 4, 6, 5, 0) 
+ CASE(MOD(Project_In_Fulfill_Date__c - DATE(1985, 1, 5), 7), 0, 0, 1, 
5, 2, 5, 3, 4, 4, 3, 5, 2, 6, 1, 0) 
- 6, 
/* Otherwise use this formula */ 
FLOOR( 
(DATEVALUE(In_Fulfillment_Out_Stamp__c ) 
-(Project_In_Fulfill_Date__c 
+ CASE(MOD(Project_In_Fulfill_Date__c - DATE(1985, 1, 5), 7), 0, 0, 1, 
6, 2, 5, 3, 4, 4, 3, 5, 2, 6, 1, 0)) 
)/7)*5 
+CASE(MOD(Project_In_Fulfill_Date__c - DATE(1985, 1, 5), 7), 0, 0, 1, 
5, 2, 5, 3, 4, 4, 3, 5, 2, 6, 1, 0) 
+CASE(MOD(DATEVALUE(In_Fulfillment_Out_Stamp__c ) - DATE(1985, 1, 5), 7), 0, 0, 1, 0, 
2, 1, 3, 2, 4, 3, 5, 4, 6, 5, 0) 
-1), 

/* If FALSE */ 
0)</formula>
        <formulaTreatBlanksAs>BlankAsZero</formulaTreatBlanksAs>
        <label>DaysInStatus InFulfillment2</label>
        <precision>18</precision>
        <required>false</required>
        <scale>2</scale>
        <type>Number</type>
        <unique>false</unique>
    </fields>
