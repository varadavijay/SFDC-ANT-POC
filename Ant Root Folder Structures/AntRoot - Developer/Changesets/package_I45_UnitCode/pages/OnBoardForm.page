	<Script>
		function inputLimiter(e, allow) {
			var AllowableCharacters = '';

			if (allow == 'Letters') {
				AllowableCharacters = ' ABCDEFGHIJKLMNOPQRSTUVWXYZabcdefghijklmnopqrstuvwxyz';
			}
			if (allow == 'Numbers') {
				AllowableCharacters = '1234567890';
			}
			if (allow == 'NameCharacters') {
				AllowableCharacters = ' ABCDEFGHIJKLMNOPQRSTUVWXYZabcdefghijklmnopqrstuvwxyz-.\'';
			}
			if (allow == 'NameCharactersAndNumbers') {
				AllowableCharacters = '1234567890 ABCDEFGHIJKLMNOPQRSTUVWXYZabcdefghijklmnopqrstuvwxyz-\'';
			}
			if (allow == 'Currency') {
				AllowableCharacters = '1234567890.';
			}

			var k = document.all ? parseInt(e.keyCode) : parseInt(e.which);
			if (k != 13 && k != 8 && k != 0) {
				if ((e.ctrlKey == false) && (e.altKey == false)) {
					return (AllowableCharacters.indexOf(String.fromCharCode(k)) != -1);
				} else {
					return true;
				}
			} else {
				return true;
			}
		}

		var j$ = jQuery.noConflict();

		function showLoadingDiv() {
			var newHeight = j$("[id$=onboardformId]").css("height");//Just shade the body, not the header
			j$("[id$=loading-curtain-div]").css("background-color", "black")
					.css("opacity", 0.35).css("height", newHeight).css("width",
							"100%").css("display", "inline");
		}
		function hideLoadingDiv() {
			j$("[id$=loading-curtain-div]").css("background-color", "black")
					.css("opacity", "1").css("height", "0px").css("width",
							"100%").css("display", "none");
		}

		var __sfdcSessionId = '{!GETSESSIONID()}';
		//http://forceadventure.wordpress.com/2013/07/19/uploading-attachments-to-salesforce-using-javascript-remoting/
		//Maximum String length of 6 million characters in apex code 
		//This means a functional limit of a file size of about 4.3 MB
		function uploadFile(inputFileId, attachmentNameId, attachmentBodyId,
				attachmentContentTypeId) {
			var input = document.getElementById(inputFileId);
			var filesToUpload = input.files;
			var maxStringSize = 6000000; //Maximum String size is 6,000,000 characters
			var maxFileSize = 4500000; //After Base64 Encoding, this is the max file size

			for ( var i = 0, onefile; onefile = filesToUpload[i]; i++) {
				var reader = new FileReader();
				// Keep a reference to the File in the FileReader so it can be accessed in callbacks
				reader.file = onefile;

				var ext = onefile.name
						.substring(onefile.name.lastIndexOf('.') + 1);
				if (ext.toLowerCase() == 'exe') {
					alert('Sorry, Executable files cannot be uploaded');
					input.value = '';
					return false;
				}

				if (onefile.size > maxFileSize) {
					alert('File must be under 4.3 MB in size.  Your file is too large.  Please try again.');
					input.value = '';
					return false;
				}
				reader.onerror = function(e) {
					switch (e.target.error.code) {
					case e.target.error.NOT_FOUND_ERR:
						alert('File Not Found!');
						break;
					case e.target.error.NOT_READABLE_ERR:
						alert('File is not readable');
						break;
					case e.target.error.ABORT_ERR:
						break; // noop
					default:
						alert('An error occurred reading this file.');
					}
					;
				};

				reader.onabort = function(e) {
					alert('File read cancelled');
				};

				reader.onload = function(e) {
					document.getElementById(attachmentNameId).value = this.file.name;
					document.getElementById(attachmentContentTypeId).value = this.file.type;
					document.getElementById(attachmentBodyId).value = window
							.btoa(this.result);
					;
				};
				reader.readAsBinaryString(onefile);
			}
		}

		//Clear attachment fields when something is attached and user didnot click on upload instead clicked on submit
		function clearAttachments() {
			//Clearing hidden controls that have html-clearfileattachment="true" attibute
			j$('input[clearfileattachment="true"]').each(function() {
				this.value = '';
				//alert( this.value );
			});

			//Clearing all file attachments content
			j$('input[type="file"]').each(function() {
				this.value = '';
				//alert( this.value );
			});
			return true;

		}
	</script>

