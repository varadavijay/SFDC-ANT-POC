													<apex:outputLink Id="displayDatabaselistAttachmentLink"
														value="/servlet/servlet.FileDownload?file={!DatabaselistFileAttachment.AttachmentId}"
														target="_blank">{!IF(DatabaselistFileAttachment.AttachmentId == NULL, '' ,DatabaselistFileAttachment.AttachmentName)}</apex:outputLink>
												</apex:actionRegion>
											</apex:outputPanel>
										</apex:pageBlockSectionItem>

										<apex:pageBlockSectionItem >
											<apex:outputLabel value="" />
											<apex:outputLabel value="Max attachment size is 4Mb" />
										</apex:pageBlockSectionItem>
										<apex:pageBlockSectionItem Id="LiabilityFormSectionItem1">
											<apex:outputLabel value="Attach signed Opt-in Liability Form" />
											<apex:outputPanel layout="block" styleClass="requiredInput">
												<apex:outputPanel id="LiablityFormRequiredAttachment"
													layout="block" styleClass="requiredBlock"
													rendered="{!NOT(ISBLANK(DatabaselistFileAttachment.AttachmentId))}" />
												<apex:inputFile Id="LiabilityFormFileUpload"
													filename="{!LiabilityFormFileAttachment.AttachmentName}"
													contentType="{!LiabilityFormFileAttachment.AttachmentContentType}"
													value="{!LiabilityFormFileAttachment.AttachmentBody}"
													onchange="uploadFile('{!$Component.LiabilityFormFileUpload}',  '{!$Component.UpLoadAssets.LiabilityFormSectionItem2.LiabilityFormattachmentName}', '{!$Component.UpLoadAssets.LiabilityFormSectionItem2.LiabilityFormattachmentBody}', '{!$Component.UpLoadAssets.LiabilityFormSectionItem2.LiabilityFormattachmentContentType}');" />
											</apex:outputPanel>
										</apex:pageBlockSectionItem>

										<apex:pageBlockSectionItem Id="LiabilityFormSectionItem2">
											<apex:outputLabel value="-" />
											<apex:outputPanel >
												<apex:actionRegion >
													<apex:commandButton action="{!LiabilityFormFileAttachment.uploadAttachment}"
														value="Upload Attachment"
														reRender="LiabilityFormattachmentId, displayLiabilityFormAttachmentLink, LiabilityFormattachmentBody"
														status="status" />
													<apex:inputHidden value="{!LiabilityFormFileAttachment.AttachmentBody}"
														html-clearfileattachment="true"
														id="LiabilityFormattachmentBody" />
													<apex:inputHidden value="{!LiabilityFormFileAttachment.AttachmentName}"
														id="LiabilityFormattachmentName" />
													<apex:inputHidden value="{!LiabilityFormFileAttachment.AttachmentContentType}"
														id="LiabilityFormattachmentContentType" />
													<apex:inputHidden value="{!LiabilityFormFileAttachment.AttachmentId}"
														Id="LiabilityFormattachmentId" />
													<apex:outputLink Id="displayLiabilityFormAttachmentLink"
														value="/servlet/servlet.FileDownload?file={!LiabilityFormFileAttachment.AttachmentId}"
														target="_blank">{!IF(LiabilityFormFileAttachment.AttachmentId == NULL, '' ,LiabilityFormFileAttachment.AttachmentName)}</apex:outputLink>
												</apex:actionRegion>
											</apex:outputPanel>
										</apex:pageBlockSectionItem>
									</apex:pageBlockSection>
								</apex:pageBlockSection>
							</apex:pageBlockSection>
						</apex:outputPanel>
					</apex:pageBlocksection>
				</apex:pageBlock>
			</apex:form>
			<div id="tooltip" style="display: none;">
				<apex:outputText value="{!$Label.GLRateHelp}" escape="false" />
			</div>
		</apex:define>
