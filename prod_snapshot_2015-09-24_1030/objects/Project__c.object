    <fields>
        <fullName>Earliest_Timestamp__c</fullName>
        <description>Should not fall through to the last NULL, so if it does, investigate</description>
        <externalId>false</externalId>
        <formula>IF(Not(ISNULL(In_Awaiting_Onboard_Stamp__c)) &amp;&amp;
   (ISNULL(In_Fulfillment_Stamp__c ) || In_Awaiting_Onboard_Stamp__c &lt; In_Fulfillment_Stamp__c) &amp;&amp;
   (ISNULL(In_Waiting_Approval_Stamp__c ) || In_Awaiting_Onboard_Stamp__c &lt; In_Waiting_Approval_Stamp__c) &amp;&amp;
   (ISNULL(In_Waiting_Resource_Stamp__c ) || In_Awaiting_Onboard_Stamp__c &lt; In_Waiting_Resource_Stamp__c) &amp;&amp;
   (ISNULL(In_Pending_Stamp__c) || In_Awaiting_Onboard_Stamp__c &lt; In_Pending_Stamp__c),
       In_Awaiting_Onboard_Stamp__c,
IF(Not(ISNULL(In_Fulfillment_Stamp__c)) &amp;&amp;
   (ISNULL(In_Awaiting_Onboard_Stamp__c ) || In_Fulfillment_Stamp__c &lt; In_Awaiting_Onboard_Stamp__c) &amp;&amp;
   (ISNULL(In_Waiting_Approval_Stamp__c ) || In_Fulfillment_Stamp__c &lt; In_Waiting_Approval_Stamp__c) &amp;&amp;
   (ISNULL(In_Waiting_Resource_Stamp__c ) || In_Fulfillment_Stamp__c &lt; In_Waiting_Resource_Stamp__c) &amp;&amp;
   (ISNULL(In_Pending_Stamp__c ) || In_Fulfillment_Stamp__c &lt; In_Pending_Stamp__c),
       In_Fulfillment_Stamp__c,
IF(Not(ISNULL(In_Waiting_Approval_Stamp__c)) &amp;&amp;
   (ISNULL(In_Awaiting_Onboard_Stamp__c ) || In_Waiting_Approval_Stamp__c &lt; In_Awaiting_Onboard_Stamp__c) &amp;&amp;
   (ISNULL(In_Fulfillment_Stamp__c ) || In_Waiting_Approval_Stamp__c &lt; In_Fulfillment_Stamp__c) &amp;&amp;
   (ISNULL(In_Waiting_Resource_Stamp__c ) || In_Waiting_Approval_Stamp__c &lt; In_Waiting_Resource_Stamp__c) &amp;&amp;
   (ISNULL(In_Pending_Stamp__c ) || In_Waiting_Approval_Stamp__c &lt; In_Pending_Stamp__c),
       In_Waiting_Approval_Stamp__c ,
IF(Not(ISNULL(In_Waiting_Resource_Stamp__c)) &amp;&amp;
   (ISNULL(In_Awaiting_Onboard_Stamp__c ) || In_Waiting_Resource_Stamp__c &lt; In_Awaiting_Onboard_Stamp__c) &amp;&amp;
   (ISNULL(In_Fulfillment_Stamp__c ) || In_Waiting_Resource_Stamp__c &lt; In_Fulfillment_Stamp__c) &amp;&amp;
   (ISNULL(In_Waiting_Approval_Stamp__c ) || In_Waiting_Resource_Stamp__c &lt; In_Waiting_Approval_Stamp__c) &amp;&amp;
   (ISNULL(In_Pending_Stamp__c ) || In_Waiting_Resource_Stamp__c &lt; In_Pending_Stamp__c),
       In_Waiting_Resource_Stamp__c ,
IF(NOT(ISNULL(In_Pending_Stamp__c)) &amp;&amp;
   (ISNULL(In_Awaiting_Onboard_Stamp__c ) || In_Pending_Stamp__c&lt; In_Awaiting_Onboard_Stamp__c) &amp;&amp;
   (ISNULL(In_Fulfillment_Stamp__c ) || In_Pending_Stamp__c&lt; In_Fulfillment_Stamp__c) &amp;&amp;
   (ISNULL(In_Waiting_Approval_Stamp__c ) || In_Pending_Stamp__c&lt; In_Waiting_Approval_Stamp__c) &amp;&amp;
   (ISNULL(In_Waiting_Resource_Stamp__c ) || In_Pending_Stamp__c&lt; In_Waiting_Resource_Stamp__c),
       In_Pending_Stamp__c,NULL
)))))</formula>
        <label>Earliest Timestamp</label>
        <required>false</required>
        <trackHistory>false</trackHistory>
        <trackTrending>false</trackTrending>
        <type>DateTime</type>
