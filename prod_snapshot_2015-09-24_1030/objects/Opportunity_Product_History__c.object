        <columns>Time_Out__c</columns>
        <columns>Duration__c</columns>
        <columns>H2GL__c</columns>
        <columns>NAME</columns>
        <filterScope>Everything</filterScope>
        <filters>
            <field>LAST_UPDATE</field>
            <operation>equals</operation>
            <value>LAST_N_DAYS:100</value>
        </filters>
        <filters>
            <field>Product_Live_DateTime__c</field>
            <operation>notEqual</operation>
        </filters>
        <filters>
            <field>Current_Product_Stage__c</field>
            <operation>contains</operation>
            <value>Live,Fulfilled</value>
        </filters>
        <filters>
            <field>OLI_Stage__c</field>
            <operation>equals</operation>
            <value>Awaiting Onboard,In Fulfillment,Waiting On Client Resource</value>
        </filters>
        <filters>
            <field>OLI_Stage__c</field>
            <operation>equals</operation>
            <value>Closed Won,Waiting On Client Approval,Pending</value>
        </filters>
        <label>All</label>
    </listViews>
