    <fields>
        <fullName>SLA_Performance_Query__c</fullName>
        <description>OR(CONTAINS(Current_Product_Stage__c,&apos;Live&apos;),CONTAINS(Current_Product_Stage__c,&apos;Fulfilled&apos;))</description>
        <externalId>false</externalId>
        <formula>IF(
AND(
NOT(ISNULL( Product_Live_DateTime__c)),
OR(NOT(ISNULL(Time_Out__c)),CONTAINS(Current_Product_Stage__c,&apos;Live&apos;),CONTAINS(Current_Product_Stage__c,&apos;Fulfilled&apos;)),
OR(CONTAINS(Current_Product_Stage__c,&apos;Live&apos;),CONTAINS(Current_Product_Stage__c,&apos;Fulfilled&apos;))
)
,true,false)</formula>
